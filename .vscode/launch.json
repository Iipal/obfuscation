    "version": "0.2.0",
    "configurations": [
        {
            "name": "C debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/obf",
            "args": ["-fow", "test1.cfg"],
            "cwd": "${workspaceFolder}",
            "MIMode": "lldb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                }
            ]
        }
    ]
}